<animation>
  <!-- JET - Jetto -->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>idle</name>
    <startframe>1</startframe>
    <endframe>60</endframe>
  </animationsubset>

  <animationsubset>
    <name>entertainment</name>
    <startframe>300</startframe>
    <endframe>359</endframe>
  </animationsubset>

  <animationsubset>
    <name>forward</name>
    <startframe>100</startframe>
    <endframe>159</endframe>
  </animationsubset>

  <animationsubset>
    <name>cursorreact</name>
    <startframe>200</startframe>
    <endframe>260</endframe>
  </animationsubset>

  <animationsubset>
    <name>squash</name>
    <startframe>400</startframe>
    <endframe>400</endframe>
  </animationsubset> 

  <animationsubset>
    <name>leap</name>
    <startframe>405</startframe>
    <endframe>405</endframe>
  </animationsubset> 

  <animationsubset>
    <name>floating</name>
    <startframe>410</startframe>
    <endframe>410</endframe>
  </animationsubset> 

  <animationsubset>
    <name>landing</name>
    <startframe>415</startframe>
    <endframe>415</endframe>
  </animationsubset> 

</animation>
