<animation>
	<!-- HUT - hatto -->

	<tickspersecond>30.0</tickspersecond>

	<animationsubset>
		<name>open</name>
		<startframe>1</startframe>
		<endframe>15</endframe>
	</animationsubset>

	<animationsubset>
		<name>close</name>
		<startframe>60</startframe>
		<endframe>75</endframe>
	</animationsubset>

	<animationsubset>
		<name>idle</name>
		<startframe>100</startframe>
		<endframe>138</endframe>
	</animationsubset>

	<animationsubset>
		<name>idlewhileopen</name>
		<startframe>160</startframe>
		<endframe>198</endframe>
	</animationsubset>
	
</animation>
