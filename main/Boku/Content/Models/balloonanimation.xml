<animation>
  <!-- BALLOON - Hobu-->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>idle</name>
    <startframe>1</startframe>
    <endframe>159</endframe>
  </animationsubset>

  <animationsubset>
    <name>entertainment</name>
    <startframe>200</startframe>
    <endframe>359</endframe>
  </animationsubset>

  <animationsubset>
    <name>entertainment2</name>
    <startframe>2000</startframe>
    <endframe>2200</endframe>
  </animationsubset>

  <animationsubset>
    <name>left</name>
    <startframe>700</startframe>
    <endframe>858</endframe>
  </animationsubset>

  <animationsubset>
    <name>right</name>
    <startframe>900</startframe>
    <endframe>1059</endframe>
  </animationsubset>

  <animationsubset>
    <name>forward</name>
    <startframe>1100</startframe>
    <endframe>1259</endframe>
  </animationsubset>

  <animationsubset>
    <name>rocket</name>
    <startframe>1300</startframe>
    <endframe>1329</endframe>
  </animationsubset>

  <animationsubset>
  <!-- For the FloatInAirChassis, backwards is used to blend in wind animation. -->
    <name>backwards</name>
    <startframe>1400</startframe>
    <endframe>1559</endframe>
  </animationsubset>

  <animationsubset>
    <name>cursorreact</name>
    <startframe>1700</startframe>
    <endframe>1859</endframe>
  </animationsubset>

</animation>
