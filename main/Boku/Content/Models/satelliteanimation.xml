<animation>
  <!-- SATELLITE - sateraito -->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>idle</name>
    <startframe>1</startframe>
    <endframe>60</endframe>
  </animationsubset>

  <animationsubset>
    <name>entertainment</name>
    <startframe>500</startframe>
    <endframe>560</endframe>
  </animationsubset>

  <animationsubset>
    <name>forward</name>
    <startframe>100</startframe>
    <endframe>159</endframe>
  </animationsubset>

  <animationsubset>
    <name>left</name>
    <startframe>200</startframe>
    <endframe>260</endframe>
  </animationsubset>

  <animationsubset>
    <name>right</name>
    <startframe>300</startframe>
    <endframe>360</endframe>
  </animationsubset>

  <animationsubset>
    <name>cursorreact</name>
    <startframe>400</startframe>
    <endframe>459</endframe>
  </animationsubset>

  <animationsubset>
    <name>rocket</name>
    <startframe>600</startframe>
    <endframe>620</endframe>
  </animationsubset>

</animation>

