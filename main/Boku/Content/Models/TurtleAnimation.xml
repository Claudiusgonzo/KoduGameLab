<animation>
  <!-- TURTLE - kamesan -->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>idle</name>
    <startframe>1</startframe>
    <endframe>60</endframe>
  </animationsubset>

  <animationsubset>
    <name>entertainment</name>
    <startframe>600</startframe>
    <endframe>740</endframe>
  </animationsubset>

  <animationsubset>
    <name>forward</name>
    <startframe>100</startframe>
    <endframe>159</endframe>
  </animationsubset>

  <animationsubset>
    <name>left</name>
    <startframe>300</startframe>
    <endframe>360</endframe>
  </animationsubset>

  <animationsubset>
    <name>right</name>
    <startframe>200</startframe>
    <endframe>260</endframe>
  </animationsubset>

  <animationsubset>
    <name>cursorreact</name>
    <startframe>400</startframe>
    <endframe>519</endframe>
  </animationsubset>

  <animationsubset>
    <name>rocket</name>
    <startframe>800</startframe>
    <endframe>820</endframe>
  </animationsubset>

  <animationsubset>
    <name>close</name>
    <startframe>900</startframe>
    <endframe>920</endframe>
  </animationsubset>

  <animationsubset>
    <name>open</name>
    <startframe>960</startframe>
    <endframe>980</endframe>
  </animationsubset>

</animation>

