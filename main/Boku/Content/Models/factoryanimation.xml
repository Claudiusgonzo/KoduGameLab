<animation>
  <!-- FACTORY - fakutori -->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>idle</name>
    <startframe>1</startframe>
    <endframe>40</endframe>
  </animationsubset>

  <animationsubset>
    <name>produce</name>
    <startframe>100</startframe>
    <endframe>140</endframe>
  </animationsubset>  

  <animationsubset>
    <name>open</name>
    <startframe>200</startframe>
    <endframe>240</endframe>
  </animationsubset>  

  <animationsubset>
    <name>produceopen</name>
    <startframe>300</startframe>
    <endframe>340</endframe>
  </animationsubset>  
 
  <animationsubset>
    <name>close</name>
    <startframe>400</startframe>
    <endframe>439</endframe>
  </animationsubset>  

  <animationsubset>
    <name>produceclose</name>
    <startframe>500</startframe>
    <endframe>540</endframe>
  </animationsubset>  

  <animationsubset>
    <name>idlewhileopen</name>
    <startframe>600</startframe>
    <endframe>640</endframe>
  </animationsubset> 

  <animationsubset>
    <name>producewhileopen</name>
    <startframe>700</startframe>
    <endframe>740</endframe>
  </animationsubset> 

</animation>
