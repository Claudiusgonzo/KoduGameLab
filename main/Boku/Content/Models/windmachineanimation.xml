<animation>
  <!-- WINDMACHINE -->

  <tickspersecond>30.0</tickspersecond>

  <animationsubset>
    <name>full_running</name>
    <startframe>1</startframe>
    <endframe>90</endframe>
  </animationsubset>

  <animationsubset>
    <name>floating_no_propeller</name>
    <startframe>100</startframe>
    <endframe>190</endframe>
  </animationsubset>

  <animationsubset>
    <name>starting_up</name>
    <startframe>200</startframe>
    <endframe>290</endframe>
  </animationsubset>

</animation>
