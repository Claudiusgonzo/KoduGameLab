<animation>
  <!-- GATE -->

	<tickspersecond>30.0</tickspersecond>
	
        <animationsubset>
		<name>open_A</name>
		<startframe>0</startframe>
		<endframe>10</endframe>
	</animationsubset>


	<animationsubset>
		<name>close_A</name>
		<startframe>10</startframe>
		<endframe>20</endframe>
	</animationsubset>


	<animationsubset>
		<name>open_B</name>
		<startframe>20</startframe>
		<endframe>30</endframe>
	</animationsubset>


	<animationsubset>
		<name>close_B</name>
		<startframe>30</startframe>
		<endframe>40</endframe>
	</animationsubset>

</animation>